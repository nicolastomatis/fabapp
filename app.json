{
  "expo": {
    "name": "fabapp",
    "slug": "fabapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "red"
    },
    "androidNavigationBar": {
      "visible": "sticky"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "red"
      },
      "package": "com.nicolastomatis.fabapp",
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8b4fe3b2-f66f-4ff4-b89e-dd2786ab2135"
      }
    }
  }
}
