{
  "cli": {
    "version": ">= 11.0.2"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "android": {
        "buildType": "app-bundle",  
        "distribution": "store"     
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "production" 
      }
    }
  }
}
